using System;

namespace primeornot
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Enter any no.:");
            int n = int.Parse(Console.ReadLine());
            int a = 0;
            for (int i = 1; i <= n; i++)
            {
                if (n % i == 0)
                {
                    a++;
                }
            }

            if (a == 2)
            {
                Console.WriteLine(n+ " is a Prime Number");
            }
            else
            {
                Console.WriteLine(n +" not a Prime Number");
            }
            Console.ReadLine();

        }
    }
}
